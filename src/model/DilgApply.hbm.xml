<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.DilgApply" table="Dilg_apply" catalog="CIS">
        <id name="Oid" type="java.lang.Integer">
            <column name="Oid" />
            <generator class="native" />
        </id>          
        <property name="student_no" type="java.lang.String">
            <column name="student_no"/>
        </property>        
        <property name="reason" type="java.lang.String">
            <column name="reason"/>
        </property>
        <property name="note" type="java.lang.String">
            <column name="note"/>
        </property>
        <property name="abs" type="java.lang.String">
            <column name="abs"/>
        </property>
        <property name="auditor" type="java.lang.String">
            <column name="auditor"/>
        </property> 
        <property name="result" type="java.lang.String">
            <column name="result"/>
        </property>
        <property name="defaultLevel" type="java.lang.String">
            <column name="defaultLevel"/>
        </property>
        <property name="realLevel" type="java.lang.String">
            <column name="realLevel"/>
        </property>
        <property name="reply" type="java.lang.String">
            <column name="reply"/>
        </property>
        <property name="cr_date" type="java.util.Date">
            <column name="cr_date"/>
        </property>
    </class>
</hibernate-mapping>